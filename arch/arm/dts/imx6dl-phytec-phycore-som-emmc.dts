/dts-v1/;

#include <arm/imx6dl.dtsi>
#include "imx6dl.dtsi"
#include "imx6qdl-phytec-phycore-som.dtsi"

/ {
	model = "Phytec phyCORE-i.MX6 DualLite/SOLO with eMMC";
	compatible = "phytec,imx6dl-pcm058-emmc", "fsl,imx6dl";
};

&ecspi1 {
	status = "okay";
};

&eeprom {
	status = "okay";
};

&ethphy {
	max-speed = <100>;
};

&fec {
	status = "okay";
};

&flash {
	status = "okay";
};

&usbh1 {
	status = "okay";
};

&usbotg {
	status = "okay";
};

&usdhc1 {
	status = "okay";

	#address-cells = <1>;
	#size-cells = <1>;

	partition@0 {
		label = "barebox";
		reg = <0x0 0xe0000>;
	};

	partition@e0000 {
		label = "barebox-environment";
		reg = <0xe0000 0x20000>;
	};
};

&usdhc4 {
	status = "okay";
};
