#include <dt-bindings/clock/ath79-clk.h>

/ {
	compatible = "qca,ar9344";

	#address-cells = <1>;
	#size-cells = <1>;

	cpus {
		#address-cells = <1>;
		#size-cells = <0>;

		cpu@0 {
			device_type = "cpu";
			compatible = "mips,mips74Kc";
			reg = <0>;
		};
	};

	ref: ref {
		compatible = "fixed-clock";
		#clock-cells = <0>;
	};

	ahb {
		compatible = "simple-bus";
		ranges;

		#address-cells = <1>;
		#size-cells = <1>;

		uart0: uart@18020000 {
			compatible = "ns16550a", "qca,ar9344-uart0";
			reg = <0x18020000 0x20>;

			reg-shift = <2>;
			reg-io-width = <4>;
			big-endian;

			status = "disabled";
		};

	};

};
